# ============================================
# 环境变量示例文件
# ============================================
# 复制此文件为 .env.local 并填入实际值
# .env.local 文件不会被提交到 Git

# 应用名称
VITE_APP_NAME=Lumina

# 应用版本
VITE_APP_VERSION=1.0.0

# 环境标识 (development | production | staging)
VITE_ENV=development

# API 基础路径
VITE_API_BASE_URL=http://localhost:3000/api

# 站点域名
VITE_SITE_HOSTNAME=http://localhost:5173

# 开发工具开关
VITE_ENABLE_DEVTOOLS=true

# Mock 数据开关
VITE_ENABLE_MOCK=true

# ============================================
# 以下为敏感配置（请勿提交到版本控制）
# ============================================
# VITE_API_KEY=your_api_key_here
# VITE_SECRET_TOKEN=your_secret_token_here

# ============================================
# Umami 分析配置
# ============================================
# 网站唯一标识（从 Umami 控制台获取）
VITE_UMAMI_WEBSITE_ID=your_website_id_here
# Umami 脚本地址
VITE_UMAMI_SCRIPT_URL=https://your-umami.com/script.js
# Umami 认证信息(用于Footer统计功能,通过API获取数据)
VITE_UMAMI_USERNAME=your_username
VITE_UMAMI_PASSWORD=your_password
# 是否启用埋点（生产环境设为 true）
VITE_UMAMI_ENABLED=false

