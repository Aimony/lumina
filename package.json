{
  "name": "lumina",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "npm run data && set \"BROWSER=C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe\"",
    "dev": "vite --host --open",
    "data": "node scripts/generate-search-index.js && node scripts/generate-graph-data.js",
    "build": "npm run data && vite build",
    "tsc": "vue-tsc -b",
    "preview": "vite preview",
    "commit": "git-cz",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "deploy": "node scripts/deploy.js",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jszip": "^3.4.0",
    "@unhead/vue": "^2.0.19",
    "@vue-office/docx": "^1.6.3",
    "@vue-office/excel": "^1.7.14",
    "@vue-office/pdf": "^2.0.10",
    "@vue-office/pptx": "^1.0.1",
    "d3": "^7.9.0",
    "echarts": "^5.6.0",
    "echarts-wordcloud": "^2.1.0",
    "flexsearch": "^0.8.212",
    "jszip": "^3.10.1",
    "mermaid": "^11.12.2",
    "shiki": "^3.20.0",
    "tailwindcss": "^4.1.18",
    "unplugin-vue-markdown": "^29.2.0",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.15.0",
    "@shikijs/markdown-it": "^3.20.0",
    "@types/d3": "^7.4.3",
    "@types/flexsearch": "^0.7.6",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-github-alerts": "^1.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "reading-time": "^1.5.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-pages": "^0.33.2",
    "vite-plugin-sitemap": "^0.8.2",
    "vue-tsc": "^3.1.4"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "prettier --write"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=22"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "none"
  }
}
